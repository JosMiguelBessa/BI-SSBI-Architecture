{
	"template": "hpigmp-*", // Template aplicado a todas as bases de dados que comecem por hpigmp-
		"mappings": // Customizações a serem feitas nos campos
		{
			"hpigmplogs": // Tipo do evento
			{
				"properties": // Listagem de todos os campos que constituem um evento do tipo hpigmplogs, bem como os seus tipos
				{
					"@timestamp":{"type":"date","format":"dateOptionalTime"}, //Definir que a data é definida de forma livre no filtro que faz o parse dos eventos
					"@version":{"type":"string"},
					"igmp_geo_ip_origin" : //Campo com várias propriedades
					{
						"properties" : 
						{
							"area_code" : {"type" : "long", "index":"not_analyzed"}, //O valor not_analyzed significa que é para considerar totalmente os dados de um campo sem os analizar. No caso de um campo string, caso não exista esta costumização, só irá considerar os dados até ao primeiro espaço
							"city_name" : {"type" : "string", "index":"not_analyzed"},
							"continent_code" : {"type" : "string", "index":"not_analyzed"},
							"country_code2" : {"type" : "string", "index":"not_analyzed"},
							"country_code3" : {"type" : "string", "index":"not_analyzed"},
							"country_name" : {"type" : "string", "index":"not_analyzed"},
							"dma_code" : {"type" : "long", "index":"not_analyzed"},
							"ip" : {"type" : "string", "index":"not_analyzed"},
							"latitude" : {"type" : "double", "index":"not_analyzed"},
							"location" : {"type" : "geo_point", "index":"not_analyzed"},
							"longitude" : {"type" : "double", "index":"not_analyzed"},
							"postal_code" : {"type" : "string", "index":"not_analyzed"},
							"real_region_name" : {"type" : "string", "index":"not_analyzed"},
							"region_name" : {"type" : "string", "index":"not_analyzed"},
							"timezone" : {"type" : "string", "index":"not_analyzed"}
						}
					},
					"igmp_geo_ip_destiny" : 
					{
						"properties" : 
						{
							"area_code" : {"type" : "long", "index":"not_analyzed"},
							"city_name" : {"type" : "string", "index":"not_analyzed"},
							"continent_code" : {"type" : "string", "index":"not_analyzed"},
							"country_code2" : {"type" : "string", "index":"not_analyzed"},
							"country_code3" : {"type" : "string", "index":"not_analyzed"},
							"country_name" : {"type" : "string", "index":"not_analyzed"},
							"dma_code" : {"type" : "long", "index":"not_analyzed"},
							"ip" : {"type" : "string", "index":"not_analyzed"},
							"latitude" : {"type" : "double", "index":"not_analyzed"},
							"location" : {"type" : "geo_point", "index":"not_analyzed"},
							"longitude" : {"type" : "double", "index":"not_analyzed"},
							"postal_code" : {"type" : "string", "index":"not_analyzed"},
							"real_region_name" : {"type" : "string", "index":"not_analyzed"},
							"region_name" : {"type" : "string", "index":"not_analyzed"},
							"timezone" : {"type" : "string", "index":"not_analyzed"}
						}
					},
					"ip_origin_igmp":{"type":"string", "index":"not_analyzed"},
					"ip_destiny_igmp":{"type":"string", "index":"not_analyzed"},
					"package_size_igmp":{"type":"string", "index":"not_analyzed"},
					"host":{"type":"string"},
					"message":{"type":"string"},
					"path":{"type":"string"},
					"type":{"type":"string"}
				}
			}		
		}
}